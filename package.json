{
  "name": "ooi",
  "description": "An add-on to find memory leaks",
  "author": "David Bruant",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "compile": "tsc Firefox/src/ts/*.ts Firefox/src/ts/**/*.ts --noEmit -t ES2016 --suppressExcessPropertyErrors --noImplicitAny --suppressImplicitAnyIndexErrors --lib es2016,dom",
    "build:main": "tsc",
    "build:ooi-panel-frame-script": "browserify Firefox/src/ts/entrypoints/ooi-panel-frame-script.ts -p tsify -o Firefox/data/ooi-panel-frame-script.js",
    "start": "jpm --addon-dir Firefox -b /home/david/Logiciel/aurora/firefox --debug run",
    "xpi": "jpm --addon-dir Firefox xpi"
  },
  "dependencies": {},
  "license": "MIT",
  "version": "0.1.0",
  "devDependencies": {
    "@types/node": "^7.0.8",
    "browserify": "^14.1.0",
    "jpm": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "tsify": "^3.0.1",
    "typescript": "^2.2.1",
    "watchify": "^3.9.0"
  }
}
